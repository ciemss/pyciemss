{
    "header": {
        "name": "Advection Model",
        "schema": "https://raw.githubusercontent.com/DARPA-ASKEM/Model-Representations/petrinet_v0.1/petrinet/petrinet_schema.json",
        "schema_name": "petrinet",
        "description": "Advection as Petrinet model created by Dan Bryce and Drisana Iverson (Mosaphir)",
        "model_version": "0.2"
    },
    "model": {
        "states": [
            {
                "id": "a_0",
                "name": "a_0",
                "description": "a at [-1.5]"
            },
            {
                "id": "a_1",
                "name": "a_1",
                "description": "a at [0.0]"
            },
            {
                "id": "a_2",
                "name": "a_2",
                "description": "a at [1.5]"
            }
        ],
        "transitions": [
            {
                "id": "r_a_0_0_boundary",
                "input": [
                    "a_0"
                ],
                "output": [],
                "properties": {
                    "name": "r_a_0_0_boundary"
                }
            },
            {
                "id": "r_a_0_1_0",
                "input": [
                    "a_1"
                ],
                "output": [
                    "a_0"
                ],
                "properties": {
                    "name": "r_a_0_1_0"
                }
            },
            {
                "id": "r_a_0_2_1",
                "input": [
                    "a_2"
                ],
                "output": [
                    "a_1"
                ],
                "properties": {
                    "name": "r_a_0_2_1"
                }
            },
            {
                "id": "r_a_0_boundary_2",
                "input": [],
                "output": [
                    "a_2"
                ],
                "properties": {
                    "name": "r_a_0_boundary_2"
                }
            }
        ]
    },
    "semantics": {
        "ode": {
            "rates": [
                {
                    "target": "r_a_0_0_boundary",
                    "expression": "(p_1)*(t)"
                },
                {
                    "target": "r_a_0_1_0",
                    "expression": "(-u/dx)*(a_0)"
                },
                {
                    "target": "r_a_0_2_1",
                    "expression": "(-u/dx)*(a_1)"
                },
                {
                    "target": "r_a_0_boundary_2",
                    "expression": "(-u/dx)*(a_2)"
                }
            ],
            "initials": [
                {
                    "target": "a_0",
                    "expression": "0.4"
                },
                {
                    "target": "a_1",
                    "expression": "1.0"
                },
                {
                    "target": "a_2",
                    "expression": "0.4"
                }
            ],
            "parameters": [
                {
                    "id": "u",
                    "value": 1.0,
                    "distribution": {
                        "type": "StandardUniform1",
                        "parameters": {
                            "minimum": 0.0,
                            "maximum": 1.0
                        }
                    }
                },
                {
                    "id": "dx",
                    "value": 1.0,
                    "distribution": {
                        "type": "StandardUniform1",
                        "parameters": {
                            "minimum": 0,
                            "maximum": 1
                        }
                    }
                },
                {
                    "id": "p_1",
                    "value": 0.01
                }
            ],
            "time": {
                "id": "t",
                "units": {
                    "expression": "day",
                    "expression_mathml": "<ci>day</ci>"
                }
            }
        }
    }
}